{
  "flows": {
    "localhost": {
      "filters": [
        {
          "type": "count"
        },
        {
          "type": "open"
        },
        {
          "type": "count"
        }
      ],
      "outputs": [
        {
          "type": "file",
          "path": "./emails/"
        }
      ]
    }
  },
  "options": {
    "disableReverseLookup": false,
    "authOptional": true,
    "loggers": [
      {
        "type": "CONSOLE",
        "level": "WARN"
      },
      { "type": "FILE", "filepath": "./test.log", "level": "TRACE" }
    ]
  }
}
